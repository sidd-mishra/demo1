    NET_CONSIDERATION NUMBER,
    NET_NET_CONSIDERATION NUMBER,
    REPORT_SCENARIO VARCHAR2(1024 CHAR),
    REPORT_SCENARIO_QUALIFIER VARCHAR2(1024 CHAR),
    SALESCREDIT NUMBER,
    SETTLEMENT_CONSIDERATION NUMBER,
    SETTLEMENT_DAYS NUMBER(10,0),
    STATE VARCHAR2(1024 CHAR),
    TRADED_FLAT_OF_ACCRUED VARCHAR2(1024 CHAR),
    VALUE_DATE TIMESTAMP_NTZ,
    EXCHANGE_SETTLEMENT_RATE VARCHAR2(1024 CHAR),
    EXCHANGE_SETTLEMENT_SENSE VARCHAR2(1024 CHAR),
    INITIAL_TRADE_DATETIME TIMESTAMP_NTZ,
    OFFER_PRICE NUMBER,
    ROOT_TRADE_VERSION NUMBER(10,0),
    IS_FLIP_TRADE VARCHAR2(1024 CHAR),
    SETTLEMENT_TO_FLIP_RATE NUMBER,
    SETTLEMENT_TO_FLIP_SENSE VARCHAR2(1024 CHAR),
    ESTIMATED_COMMISSION NUMBER,
    IS_SWING_TRADE VARCHAR2(1024 CHAR),
    TRADE_SOURCE VARCHAR2(1024 CHAR),
    CPTY_TRADER_ID VARCHAR2(1024 CHAR),
    AMENDED_BY VARCHAR2(1024 CHAR),
    CROSSING_ORDER_ID VARCHAR2(1024 CHAR),
    SHORT_SELL VARCHAR2(1024 CHAR),
    BAD_SHORT_SELL VARCHAR2(1024 CHAR),
    CONTRIBUTOR_ID VARCHAR2(1024 CHAR),
    REPORT_NOTES VARCHAR2(1024 CHAR),
    REPORT_PRICE_TYPE VARCHAR2(1024 CHAR),
    REPORT_FLAGS VARCHAR2(1024 CHAR),
    COMMISSION_ARG NUMBER,
    COMMISSION_METHOD VARCHAR2(1024 CHAR),
    MARKUP_ARG NUMBER,
    MARKUP_METHOD VARCHAR2(1024 CHAR),
    SALESCREDIT_ARG NUMBER,
    SALESCREDIT_METHOD VARCHAR2(1024 CHAR),
    BROKERAGE_ARG NUMBER,
