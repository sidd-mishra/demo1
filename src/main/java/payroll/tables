    REPORT_DESTINATION_ID VARCHAR2(1024 CHAR),
    PAYMENT_METHOD_ID VARCHAR2(1024 CHAR),
    POSITION_CURRENCY_ID VARCHAR2(1024 CHAR),
    REPRESENTATIVE_ID VARCHAR2(1024 CHAR),
    SETTLEMENT_CURRENCY_ID VARCHAR2(1024 CHAR),
    TRADER VARCHAR2(1024 CHAR),
