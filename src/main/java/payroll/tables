    ORDER_AVG_PRICE VARCHAR2(1024 CHAR),
    CLEARING_FLAGS VARCHAR2(1024 CHAR),
    ACT_FIRM_ROLE VARCHAR2(1024 CHAR),
    CLEARING_CODE VARCHAR2(1024 CHAR),
