CREATE OR REPLACE TABLE Hub_Trade_Sum (
    LOAD_TIMESTAMP TIMESTAMP_NTZ,
    HK_TRADE_ID VARCHAR2(32 CHAR) PRIMARY KEY,
    TRADE_ID VARCHAR2(100 CHAR),
    RECORD_SOURCE VARCHAR2(100 CHAR),
    ETL_BATCH_ID VARCHAR2(512 CHAR)
);

CREATE OR REPLACE TABLE Sat_Pri_Trade_Sum (
    HASH_DIFF VARCHAR2(32 CHAR) PRIMARY KEY,
    LOAD_TIMESTAMP TIMESTAMP_NTZ,
    RECORD_SOURCE VARCHAR2(100 CHAR),
    ETL_BATCH_ID VARCHAR2(512 CHAR),
    HK_TRADE_ID VARCHAR2(32 CHAR),
    VERSION VARCHAR2(1024 CHAR),
    INSTRUMENT_CODE VARCHAR2(1024 CHAR),
    DEALT_PRICE NUMBER,
    NET_NET_PRICE NUMBER,
    DEALT_CURRENCY_ID VARCHAR2(1024 CHAR),
    BUY_SELL VARCHAR2(1024 CHAR),
    QUANTITY NUMBER,
    TRADE_DATETIME TIMESTAMP_NTZ,
    ORDER_ID VARCHAR2(1024 CHAR),
    PARENT_ORDER_ID VARCHAR2(1024 CHAR),
    COUNTERPARTY_CODE VARCHAR2(1024 CHAR),
    GROSS_CONSIDERATION NUMBER,
    STATE VARCHAR2(1024 CHAR),
    SETTLEMENT_CURRENCY_ID VARCHAR2(1024 CHAR),
    TRADER VARCHAR2(1024 CHAR),
    COMMISSION NUMBER,
    ENTERED_DATETIME TIMESTAMP_NTZ,
    DEALING_CAPACITY VARCHAR2(1024 CHAR),
    ROOT_TRADE_VERSION NUMBER(10,0),
    BOOK_CODE VARCHAR2(1024 CHAR)
);

CREATE OR REPLACE TABLE Sat_Sec_Trade_Sum (
    HASH_DIFF VARCHAR2(32 CHAR) PRIMARY KEY,
    LOAD_TIMESTAMP TIMESTAMP_NTZ,
